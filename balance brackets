
                }
                if(isBalanced(popped, now))
                    continue;
                else
                {
                    no = 1;
                    break;
                }
            }
        }
        if(s.size() != 0)
            no = 1;
        if(no)
            cout<<"NO\n";
        else
        {
            cout<<"YES\n";
        }
    }
    return 0;
}	

